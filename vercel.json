{
  "version": 2,
  "builds": [
    {
      "src": "dms/",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "dms/static/$1"
    },
    {
      "src": "/media/(.*)",
      "dest": "dms/media/$1"
    },
    {
      "src": "/admin/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=0, must-revalidate"
      },
      "dest": "/dms/admin/$1"
    },
    {
      "src": "/(.*)",
      "dest": "dms/dms/wsgi.py"
    }
  ],
  "build": {
    "env": {
      "DJANGO_SETTINGS_MODULE": "dms.settings"
    }
  }
}
