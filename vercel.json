{
  "builds": [
    {
      "src": "dms/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "300mb",
        "serverless": {
          "memory": 1024,
          "maxDuration": 30
        },
        "runtime": "python3.11",
        "requirements": "requirements.txt"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dms/wsgi.py"
    }
  ]
}